<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<BlazorDisableThrowNavigationException>true</BlazorDisableThrowNavigationException>

		<PublishAot>false</PublishAot>
		<PublishSingleFile>false</PublishSingleFile>
		<SelfContained>false</SelfContained>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="LiveStreamingServerNet" Version="0.33.1" />
		<PackageReference Include="LiveStreamingServerNet.AdminPanelUI" Version="0.33.1" />
		<PackageReference Include="LiveStreamingServerNet.Flv" Version="0.33.1" />
		<PackageReference Include="LiveStreamingServerNet.Standalone" Version="0.33.1" />
		<PackageReference Include="LiveStreamingServerNet.StreamProcessor.AspNetCore" Version="0.33.1" />
		<PackageReference Include="MailerSendNetCore" Version="0.1.0" />
		<PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="10.0.1" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="10.0.1">
		  <PrivateAssets>all</PrivateAssets>
		  <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="10.0.1" />
		<PackageReference Include="MudBlazor" Version="8.15.0" />
		<PackageReference Include="Scalar.AspNetCore" Version="2.11.6" />
		<PackageReference Include="Telegram.Bot" Version="22.7.7-dev.1" />
	</ItemGroup>

</Project>
