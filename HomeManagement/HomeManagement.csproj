<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>

		<PublishAot>false</PublishAot>
		<PublishSingleFile>false</PublishSingleFile>
		<SelfContained>false</SelfContained>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="LiveStreamingServerNet" Version="0.31.1" />
		<PackageReference Include="LiveStreamingServerNet.AdminPanelUI" Version="0.31.1" />
		<PackageReference Include="LiveStreamingServerNet.Flv" Version="0.31.1" />
		<PackageReference Include="LiveStreamingServerNet.Standalone" Version="0.31.1" />
		<PackageReference Include="LiveStreamingServerNet.StreamProcessor.AspNetCore" Version="0.31.1" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="10.0.0-rc.1.25451.107">
		  <PrivateAssets>all</PrivateAssets>
		  <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="10.0.0-rc.1.25451.107" />
		<PackageReference Include="MudBlazor" Version="8.12.0" />
	</ItemGroup>

</Project>
